// Loop through IDs from answerB1A to answerB14A
for (let i = 1; i <= 14; i++) {
    // Construct the element ID
    let elementId = 'answerB' + i + 'A';

    // Find the element by ID
    let element = document.querySelector('#' + elementId);

    // Check if the element exists
    if (element) {
        // Create a new mouse click event
        let clickEvent = new MouseEvent('click', {
            bubbles: true,
            cancelable: true,
            view: window
        });

        // Dispatch the click event on the element
        element.dispatchEvent(clickEvent);

        // Output a message to the console indicating the click
        console.log('Left clicked on element with ID ' + elementId);
    } else {
        // Output a message to the console indicating the element was not found
        console.log('Element with ID ' + elementId + ' not found');
    }
}

// Find and click on element with ID answerB15C
let answerB15C = document.querySelector('#answerB15C');
if (answerB15C) {
    let clickEvent = new MouseEvent('click', {
        bubbles: true,
        cancelable: true,
        view: window
    });

    // Dispatch the click event on the answerB15C element
    answerB15C.dispatchEvent(clickEvent);

    // Output a message to the console indicating the click
    console.log('Left clicked on element with ID answerB15C');
} else {
    // Output a message to the console indicating answerB15C was not found
    console.log('Element with ID answerB15C not found');
}

